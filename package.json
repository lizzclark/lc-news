{
  "name": "LC-News",
  "version": "1.0.0",
  "description": "## RESTful News API",
  "main": "index.js",
  "scripts": {
    "test": "psql -f db/setup_test.sql && mocha spec",
    "lint": "eslint ./",
    "pre-commit": "npm t",
    "posttest": "npm run lint",
    "make-migration": "knex migrate:make",
    "migrate-latest": "knex migrate:latest",
    "migrate-rollback": "knex migrate:rollback",
    "setup-dbs": "psql -f db/setup.sql && psql -f db/setup_test.sql",
    "seed": "knex seed:run",
    "dev": "nodemon listen.js",
    "start": "node listen.js",
    "seed:prod": "NODE_ENV=production DB_URL=$(heroku config:get DATABASE_URL) knex seed:run",
    "migrate:latest:prod": "NODE_ENV=production DB_URL=$(heroku config:get DATABASE_URL) knex migrate:latest",
    "migrate:rollback:prod": "NODE_ENV=production DB_URL=$(heroku config:get DATABASE_URL) knex migrate:rollback"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lizzclark/lc-news.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lizzclark/lc-news/issues"
  },
  "homepage": "https://github.com/lizzclark/lc-news#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "knex": "^0.15.2",
    "pg": "^7.8.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^1.1.4",
    "nodemon": "^1.18.9",
    "supertest": "^3.4.2"
  }
}
